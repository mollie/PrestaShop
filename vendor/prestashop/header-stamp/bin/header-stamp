$autoloadFiles = [__DIR__ . '/../../../autoload.php', __DIR__ . '/../vendor/autoload.php'];
foreach ($autoloadFiles as $autoloadFile) {
    if (\file_exists($autoloadFile)) {
        require_once $autoloadFile;
    }
}
use MolliePrefix\PrestaShop\HeaderStamp\Command\UpdateLicensesCommand;
use MolliePrefix\Symfony\Component\Console\Application;
$application = new \MolliePrefix\Symfony\Component\Console\Application('header-stamp', '1.4.0');
$command = new \MolliePrefix\PrestaShop\HeaderStamp\Command\UpdateLicensesCommand();
$application->add($command);
$application->setDefaultCommand($command->getName());
$application->run();
